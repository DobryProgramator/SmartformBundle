{
    "name": "dobryprogramator/smartform-bundle",
    "description": "Bundle to connect Symfony with www.smartform.cz",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Lukáš Holeczy",
            "email": "info@lukasholeczy.eu"
        }
    ],
    "require": {
        "php": "^7.4",
        "holicz/simple-exception": "^1.1",
        "symfony/form": "^3.4|^4.4|^5.0",
        "symfony/validator": "^3.4|^4.4|^5.0",
        "thecodingmachine/safe": "^1.1",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "doctrine/doctrine-bundle": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "DobryProgramator\\SmartformBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
