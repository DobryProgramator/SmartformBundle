           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="DobryProgramator\SmartformBundle\Form\DataMapper\SmartformAddressMapper"
                 class="DobryProgramator\SmartformBundle\Form\DataMapper\SmartformAddressMapper"
